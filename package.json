  "scripts": {
    "start": "npm run develop --verbose",
    "mirror": "rsync --verbose --archive --progress --exclude='*.DS_Store' src/images/* '/Volumes/GoogleDrive/Shared drives/Aura Website/Website Mirror/images' && rsync --verbose --archive --progress --exclude='*.DS_Store' src/resources/* '/Volumes/GoogleDrive/Shared drives/Aura Website/Website Mirror/resources' && rsync --verbose --archive --progress --exclude='*.DS_Store' src/intl/* '/Volumes/GoogleDrive/Shared drives/Aura Website/Website Mirror/intl'",
    "startc": "npm run develop-clean",
    "build": "gatsby build --log-pages",
    "build-clean": "rm -rf public && rm -rf .cache && GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "deploy-preview": "netlify deploy",
    "deploy": "netlify deploy --prod",
    "develop": "gatsby develop",
    "develop-clean": "rm -rf public && rm -rf .cache && gatsby develop",
    "serve": "gatsby serve",
    "format": "prettier --write '**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
